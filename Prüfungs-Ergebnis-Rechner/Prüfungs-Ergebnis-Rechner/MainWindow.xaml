<Window x:Class="Prüfungs_Ergebnis_Rechner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prüfungs_Ergebnis_Rechner"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="Berechnungs_Feld" Width="400" Margin="390,0,10,0" Height="400"/>
        <ListBox x:Name="Tester_Liste" d:ItemsSource="{d:SampleData ItemCount=5}" Width="350" Margin="0,0,450,0" Height="400">
            <ComboBoxItem/>
        </ListBox>

    </Grid>
</Window>
